<div class='main'>
  <!-- header -->
  <div class='header-container'>
    <%- include('../common/header/index', {headerData: appData.headerData}) %>
  </div>

  <!-- content -->
  <div class='content-container'>
    <amp-list credentials="include"
      width='100'
      height='100'
      noloading
      src='<%- appData.listUrl %>'
      items=''
      layout='responsive'>
      <div placeholder></div>
      <template type="amp-mustache">
          <div class='item'>
            <div class='icon'>
              <i class='htlf-hotel'></i>
            </div>
            <div class='info'>
              <span class='city'>{{word}}<span class='position'></span></span>
              <br>
              <span class='city-info'>{{cityname}}, {{provincename}}, {{countryname}}</span>
            </div>
          </div>
      </template>
    </amp-list>
  </div>

  <!-- footer -->
  <div class='footer-container'>
    <%- include('../common/footer/index', {footerData: appData.footerData}) %>
  </div>

</div>




